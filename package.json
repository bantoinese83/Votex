{
  "name": "votex-template",
  "version": "1.0.0",
  "description": "Votex template with Go backend and SvelteKit frontend",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && go run main.go",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "test": "npm run test:frontend",
    "test:frontend": "cd frontend && npm run test",
    "lint": "npm run lint:frontend",
    "lint:frontend": "cd frontend && npm run lint",
    "format": "npm run format:frontend",
    "format:frontend": "cd frontend && npm run format",
    "storybook": "cd frontend && npm run storybook",
    "db:push": "cd frontend && npm run db:push",
    "db:studio": "cd frontend && npm run db:studio"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["votex", "go", "sveltekit", "fullstack"],
  "author": "",
  "license": "MIT"
} 